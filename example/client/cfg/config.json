{
  "client": {
    "user": {
      "transports": {
        "grpc": {
          "proto": "io/restorecommerce/user.proto",
          "protoRoot": "../../protos/",
          "package": "io.restorecommerce.user",
          "service": "User",
          "timeout": 3000
        }
      },
      "endpoints": {
        "get": {
          "loadbalancer": {
            "name": "roundRobin"
          },
          "publisher": {
            "name": "static",
            "instances": ["grpc://localhost:50051"]
          }
        },
        "register": {
          "loadbalancer": {
            "name": "random",
            "seed": 1
          },
          "publisher": {
            "name": "static",
            "instances": ["grpc://localhost:50051"]
          }
        }
      }
    }
  }
}
