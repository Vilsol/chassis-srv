{
  "client": {
    "test": {
      "logger": {
        "console": {
          "handleExceptions": false,
          "level": "error",
          "colorize": true,
          "prettyPrint": true
        }
      },
      "transports": {
        "grpc": {
          "proto": "test/test.proto",
          "package": "test",
          "service": "Test",
          "timeout": 3000
        }
      },
      "endpoints": {
        "test": {
          "loadbalancer": {
            "name": "random",
            "seed": 0
          },
          "publisher": {
            "name": "static",
            "instances": ["grpc://localhost:50051"]
          }
        },
        "notImplemented": {
          "loadbalancer": {
            "name": "roundRobin"
          },
          "publisher": {
            "name": "static",
            "instances": ["grpc://localhost:50051"]
          }
        },
        "throw": {
          "loadbalancer": {
            "name": "roundRobin"
          },
          "publisher": {
            "name": "static",
            "instances": ["grpc://localhost:50051"]
          }
        }
      }
    }
  },
  "server": {
    "logger": {
      "console": {
        "handleExceptions": false,
        "level": "error",
        "colorize": true,
        "prettyPrint": true
      }
    },
    "events": {
      "provider": {
        "name": "kafka",
        "proto": ["io/restorecommerce/event.proto", "../test/test.proto"],
        "protoRoot": "../protos/",
        "groupId": "restore-chassis-test-server",
        "clientId": "restore-chassis-test-server",
        "connectionString": "localhost:9092",
        "message": "io.restorecommerce.event.Event",
        "messages": {
          "test": {
            "testEvent": "test.TestEvent"
          }
        }
      }
    },
    "endpoints": {
      "test": {
        "transport": ["grpc"]
      },
      "throw": {
        "transport": ["grpc"]
      },
      "notImplemented": {
        "transport": ["grpc"]
      }
    },
    "transports": [{
      "name": "grpc",
      "config": {
        "proto": "test/test.proto",
        "package": "test",
        "service": "Test",
        "addr": "localhost:50051"
      }
    }]
  }
}
