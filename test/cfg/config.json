{
  "database": {
    "arango": {
      "provider": "arango",
      "host": "127.0.0.1",
      "port": 8529,
      "database": "chassis-test",
      "autoCreate": true
    }
  },
  "client": {
    "test": {
      "logger": {
        "console": {
          "handleExceptions": false,
          "level": "error",
          "colorize": true,
          "prettyPrint": true
        }
      },
      "transports": {
        "grpc": {
          "service": "test.Test",
          "timeout": 3000
        }
      },
      "loadbalancer": {
        "name": "roundRobin"
      },
      "publisher": {
        "name": "static",
        "instances": ["grpc://localhost:50051"]
      },
      "endpoints": {
        "test": {
          "loadbalancer": {
            "name": "random",
            "seed": 0
          },
          "publisher": {
            "name": "static",
            "instances": ["grpc://localhost:50051"]
          }
        },
        "notImplemented": {
          "loadbalancer": {
            "name": "roundRobin"
          },
          "publisher": {
            "name": "static",
            "instances": ["grpc://localhost:50051"]
          }
        },
        "throw": {}
      }
    },
    "stream": {
      "logger": {
        "console": {
          "handleExceptions": false,
          "level": "error",
          "colorize": true,
          "prettyPrint": true
        }
      },
      "transports": {
        "grpc": {
          "service": "test.Stream",
          "timeout": 3000
        }
      },
      "loadbalancer": {
        "name": "roundRobin"
      },
      "publisher": {
        "name": "static",
        "instances": ["grpc://localhost:50051"]
      },
      "endpoints": {
        "biStream": {},
        "requestStream": {},
        "responseStream": {}
      }
    }
  },
  "server": {
    "logger": {
      "console": {
        "handleExceptions": false,
        "level": "error",
        "colorize": true,
        "prettyPrint": true
      }
    },
    "events": {
      "provider": {
        "name": "kafka",
        "groupId": "restore-chassis-test-server",
        "clientId": "restore-chassis-test-server",
        "connectionString": "localhost:9092"
      }
    },
    "services": {
      "test": {
        "test": {
          "transport": ["grpcTest"]
        },
        "throw": {
          "transport": ["grpcTest"]
        },
        "notImplemented": {
          "transport": ["grpcTest"]
        }
      },
      "stream": {
        "biStream": {
          "transport": ["grpcTest"]
        },
        "requestStream": {
          "transport": ["grpcTest"]
        },
        "responseStream": {
          "transport": ["grpcTest"]
        }
      }
    },
    "transports": [{
      "name": "grpcTest",
      "provider": "grpc",
      "services": {
        "test": "test.Test",
        "stream": "test.Stream"
      },
      "addr": "localhost:50051"
    }]
  }
}
