{
  "database": {
    "gss": {
      "provider": "gss",
      "host": "127.0.0.1",
      "database": "restorecommerce",
      "autoCreate": false
    }
  },
  "client": {
    "test": {
      "logger": {
        "console": {
          "handleExceptions": false,
          "level": "error",
          "colorize": true,
          "prettyPrint": true
        }
      },
      "transports": {
        "grpc": {
          "service": "test.Test",
          "timeout": 3000
        }
      },
      "endpoints": {
        "test": {
          "loadbalancer": {
            "name": "random",
            "seed": 0
          },
          "publisher": {
            "name": "static",
            "instances": ["grpc://localhost:50051"]
          }
        },
        "notImplemented": {
          "loadbalancer": {
            "name": "roundRobin"
          },
          "publisher": {
            "name": "static",
            "instances": ["grpc://localhost:50051"]
          }
        },
        "throw": {
          "loadbalancer": {
            "name": "roundRobin"
          },
          "publisher": {
            "name": "static",
            "instances": ["grpc://localhost:50051"]
          }
        }
      }
    }
  },
  "server": {
    "logger": {
      "console": {
        "handleExceptions": false,
        "level": "error",
        "colorize": true,
        "prettyPrint": true
      }
    },
    "events": {
      "provider": {
        "name": "kafka",
        "groupId": "restore-chassis-test-server",
        "clientId": "restore-chassis-test-server",
        "connectionString": "localhost:9092"
      }
    },
    "endpoints": {
      "test": {
        "transport": ["grpc"]
      },
      "throw": {
        "transport": ["grpc"]
      },
      "notImplemented": {
        "transport": ["grpc"]
      }
    },
    "transports": [{
      "name": "grpc",
      "service": "test.Test",
      "addr": "localhost:50051"
    }]
  }
}
